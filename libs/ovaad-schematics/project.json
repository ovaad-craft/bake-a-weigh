{
  "name": "ovaad-schematics",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/ovaad-schematics/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/ovaad-schematics",
        "main": "libs/ovaad-schematics/src/index.ts",
        "tsConfig": "libs/ovaad-schematics/tsconfig.lib.json",
        "assets": [
          "libs/ovaad-schematics/*.md",
          {
            "input": "./libs/ovaad-schematics/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "./libs/ovaad-schematics/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./libs/ovaad-schematics",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./libs/ovaad-schematics",
            "glob": "executors.json",
            "output": "."
          }
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/ovaad-schematics/jest.config.ts"
      }
    }
  }
}
